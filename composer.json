{
    "name": "vinkla/extended-api",
    "license": "MIT",
    "keywords": [
        "api",
        "extended",
        "wordpress"
    ],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "homepage": "https://github.com/vinkla"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/collections": "^11.0",
        "illuminate/validation": "^11.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "roots/wordpress-no-content": "^6.3.1",
        "symfony/var-dumper": "^7.0"
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^7.0)."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Extended\\API\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Extended\\Tests\\API\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "roots/wordpress-core-installer": true
        }
    }
}
